
{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["transferId", "status", "fromAccount", "toAccount", "amount"],
  "properties": {
    "transferId": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": ["SUCCESS", "FAILED"]
    },
    "fromAccount": {
      "type": "string"
    },
    "toAccount": {
      "type": "string"
    },
    "amount": {
      "type": "number",
      "minimum": 0.01
    }
  }
}

